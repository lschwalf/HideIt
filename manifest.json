{
  "name": "HideIt",
  "version": "1.0.0",
  "description": "Removes the content of the Reddit homepage",
  "content_scripts": [{
      "matches": ["*://*.reddit.com/*"],
      "js": ["jquery.min.js", "remove.js"],
      "run_at": "document_start"
  }],
  "icons": {
    "16": "images/icon-red.png",
    "48": "images/icon-red.png",
    "128": "images/icon-red.png"
  },
  "manifest_version": 2
}
